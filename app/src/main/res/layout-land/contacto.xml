<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical"
    >


    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/img_logo"
        android:src="@drawable/loog_murgi"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_murgi"
        android:layout_marginBottom="32dp"
        android:layout_marginTop="8dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ies Murgi"
        android:textSize="40dp"
        android:id="@+id/txt_murgi"
        android:layout_marginStart="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="40dp" />


    <ImageView
        android:layout_width="25dp"
        android:layout_height="24dp"
        android:elevation="8dp"
        android:src="@drawable/ic_telefono"
        android:id="@+id/imageView7"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contacto: "
        android:textSize="20dp"
        android:id="@+id/textView8"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+34 950481000"
        android:textSize="20dp"
        android:id="@+id/textView9"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="28dp"
        android:src="@drawable/ic_fax"
        android:id="@+id/imageView3"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginStart="24dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fax:"
        android:textSize="20dp"
        android:id="@+id/textView10"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="950323232"
        android:textSize="20dp"
        android:id="@+id/textView11"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginEnd="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginStart="8dp" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/web"
        android:id="@+id/imageView4"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        android:layout_marginBottom="24dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="web:"
        android:textSize="20dp"
        android:id="@+id/textView12"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="126dp"
        android:layout_height="42dp"
        android:text="www.iesmurgi.org"
        android:linksClickable="true"
        android:autoLink="web"
        android:textSize="15dp"
        android:id="@+id/textView14"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="24dp" />


    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/ic_mail"
        android:id="@+id/imageView5"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        android:layout_marginBottom="32dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textSize="20dp"
        android:id="@+id/textView13"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="24dp" />

    <TextView
        android:layout_width="131dp"
        android:layout_height="36dp"
        android:text="dpto.informatica@iesmurgi.org"
        android:textSize="15dp"
        android:id="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="24dp" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="28dp"
        android:src="@drawable/usuarios"
        android:id="@+id/imageView6"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginEnd="16dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginStart="24dp" />


    <TextView
        android:layout_width="150dp"
        android:layout_height="23dp"
        android:text="Desarrolladores: "
        android:textSize="20dp"
        android:id="@+id/textView16"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintRight_toLeftOf="@+id/guideline4"
        android:layout_marginEnd="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="40dp"
        android:layout_marginStart="24dp"
        app:layout_constraintHorizontal_bias="0.4" />

    <TextView
        android:layout_width="148dp"
        android:layout_height="44dp"
        android:textSize="14dp"
        android:text="Luis, Miguel, juanjo y chema"
        android:id="@+id/textView17"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="16dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="199dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="199dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        app:layout_constraintGuide_begin="289dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="289dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        app:layout_constraintGuide_begin="434dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="434dp" />


</android.support.constraint.ConstraintLayout>
